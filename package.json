{
  "name": "type-plus",
  "version": "1.0.0",
  "description": "Provides additional types for `typescript`.",
  "homepage": "https://github.com/unional/type-plus",
  "bugs": {
    "url": "https://github.com/unional/type-plus/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/type-plus.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "files": [
    "lib",
    "libm",
    "src"
  ],
  "main": "lib/index.js",
  "module": "libm/index.js",
  "jsnext:main": "libm/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.es5.json && tsc -p tsconfig.esnext.json",
    "clean": "rimraf lib libm",
    "coverage": "jest --coverage",
    "codecov": "codecov",
    "dc": "yarn depcheck",
    "depcheck": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "eslint \"src/**/*.ts\"",
    "semantic-release": "semantic-release",
    "test": "jest --reporters=default",
    "verify": "yarn lint && yarn build && yarn dc && npx size-limit && yarn coverage --reporters=default",
    "verify:ci": "yarn lint && yarn build && yarn dc && npx size-limit && yarn coverage --maxWorkers=2",
    "watch": "jest --watch"
  },
  "dependencies": {
    "unpartial": "^0.6.3"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.0.2",
    "@types/jest": "^24.0.17",
    "@unional/devpkg-node": "^1.4.0",
    "assertron": "^7.1.2",
    "codecov": "^3.5.0",
    "eslint-plugin-harmony": "^2.0.4",
    "jest": "^24.8.0",
    "semantic-release": "^15.13.19",
    "typescript": "^3.5.3"
  },
  "size-limit": [
    {
      "limit": "5 kb"
    }
  ]
}
