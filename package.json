{
  "name": "type-plus",
  "version": "0.0.0-development",
  "description": "Provides additional types for TypeScript.",
  "homepage": "https://github.com/unional/type-plus",
  "bugs": {
    "url": "https://github.com/unional/type-plus/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/type-plus.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "jsnext:main": "esm/index.js",
  "typings": "cjs/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "ts"
  ],
  "scripts": {
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:doc": "typedoc",
    "clean": "rimraf cjs esm coverage lib libm || true",
    "coverage": "jest --coverage",
    "depcheck": "depcheck",
    "lint": "eslint --ext=ts,js .",
    "nuke": "yarn clean && rimraf node_modules",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "size": "size-limit",
    "test": "jest",
    "test:types": "tsc",
    "verify": "npm-run-all clean -p build depcheck lint coverage -p size",
    "watch": "yarn test --watch"
  },
  "dependencies": {
    "tersify": "^3.10.0",
    "unpartial": "^0.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@unional/fixture": "^2.0.0",
    "assertron": "^9.2.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.17.0",
    "eslint-plugin-harmony": "^7.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "rimraf": "^3.0.2",
    "satisfier": "^5.2.0",
    "size-limit": "^7.0.8",
    "ts-jest": "^28.0.4",
    "tsc-esm-fix": "^2.17.0",
    "typedoc": "^0.22.17",
    "typedoc-plugin-extras": "^2.2.3",
    "typescript": "^4.7.3"
  },
  "size-limit": [
    {
      "limit": "15 kb"
    }
  ],
  "packageManager": "yarn@3.2.1"
}
