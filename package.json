{
  "name": "type-plus",
  "version": "0.0.0-development",
  "description": "Provides additional types for TypeScript.",
  "homepage": "https://github.com/unional/type-plus",
  "bugs": {
    "url": "https://github.com/unional/type-plus/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/type-plus.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "jsnext:main": "esm/index.js",
  "typings": "cjs/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "ts"
  ],
  "scripts": {
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:doc": "typedoc",
    "clean": "rimraf cjs esm coverage lib libm",
    "coverage": "jest --coverage",
    "depcheck": "yarn dependency-check",
    "dependency-check": "run-p dependency-check:unused dependency-check:missing",
    "dependency-check:missing": "dependency-check . --missing --no-dev",
    "dependency-check:unused": "dependency-check . --unused --no-dev -i typescript",
    "lint": "eslint --ext=ts,js .",
    "nuke": "yarn clean && rimraf node_modules",
    "release": "npx semantic-release",
    "size-limit": "size-limit",
    "test": "jest",
    "test:types": "tsc",
    "verify": "run-p verify:build lint test:types coverage",
    "verify:build": "npm-run-all clean build --parallel dependency-check size-limit",
    "watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "tersify": "^3.10.0",
    "unpartial": "^0.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.17.12",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@unional/fixture": "^2.0.0",
    "assertron": "^9.2.3",
    "dependency-check": "^4.1.0",
    "eslint": "^8.17.0",
    "eslint-plugin-harmony": "^7.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-progress-tracker": "^3.0.4",
    "jest-validate": "^28.1.0",
    "jest-watch-repeat": "^2.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "satisfier": "^5.2.0",
    "size-limit": "^7.0.8",
    "ts-jest": "^28.0.4",
    "typedoc": "^0.22.17",
    "typedoc-plugin-extras": "^2.2.3",
    "typescript": "^4.7.3"
  },
  "size-limit": [
    {
      "limit": "15 kb"
    }
  ]
}
