{
  "extends": "./tsconfig.esm.json",
  "compilerOptions": {
    "lib": [
      "es2015",
      "esnext.bigint"
    ],
    "noEmit": true
  },
  "include": [
    "ts",
    "typings"
  ],
  "typedocOptions": {
    "customTitle": "Additional types and utils for TypeScript",
    "customTitleLink": "https://github.com/unional/type-plus",
    "entryPoints": [
      "ts/index.ts"
    ],
    "out": "docs"
  }
}
