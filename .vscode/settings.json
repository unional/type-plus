{
	"[markdown]": {
		"editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"cSpell.words": [
		"ajafff",
		"Birkeland",
		"Cernicova",
		"Colthorp",
		"Constantiner",
		"Dragomir",
		"Endo",
		"endofunctor",
		"endofunctors",
		"gcanti",
		"istype",
		"jcalz",
		"jsnext",
		"keyof",
		"libm",
		"Meinhardt",
		"miku",
		"overrider",
		"pinst",
		"repobuddy",
		"Significand",
		"snyk",
		"tersify",
		"typelevel",
		"typepark",
		"unpartial",
		"unshift"
	],
	"markdown.extension.toc.levels": "2..6",
	"markdown.extension.toc.omittedFromToc": {
		"README.md": [
			"## Table of Contents",
			"## Installation"
		]
	},
	"eslint.enable": true,
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/.hg/store/**": true,
		"**/.progress/**": true,
		"**/node_modules/*/**": true,
		".github": true,
		"cjs": true,
		"coverage": true,
		"esm": true,
		"scripts": true
	},
	"json.schemas": [
		{
			"fileMatch": [
				"tslint.json"
			],
			"url": "http://json.schemastore.org/tslint"
		},
		{
			"fileMatch": [
				"tsconfig.*.json"
			],
			"url": "http://json.schemastore.org/tsconfig"
		}
	],
	"markdown.extension.toc.orderedList": true,
	"tsimporter.filesToExclude": [
		"**/cjs",
		"**/esm",
		"**/tslib"
	],
	"typescript.tsdk": "node_modules/typescript/lib"
}
