{
	"[markdown]": {
		"editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome",
		"editor.formatOnSave": true,

	},
	"editor.codeActionsOnSave":  {
		"source.organizeImports.biome": "explicit"
	},
	"cSpell.words": [
		"ajafff",
		"Alpysbayev",
		"astro",
		"astrojs",
		"biomejs",
		"Birkeland",
		"Cernicova",
		"Colthorp",
		"Constantiner",
		"Dragomir",
		"Endo",
		"Endofunction",
		"endofunctor",
		"endofunctors",
		"esbuild",
		"gcanti",
		"istype",
		"jcalz",
		"jsnext",
		"keyof",
		"libm",
		"Meinhardt",
		"miku",
		"Nurbol",
		"overrider",
		"pinst",
		"repobuddy",
		"Significand",
		"snyk",
		"tersify",
		"tsbuildinfo",
		"tsconfigs",
		"typelevel",
		"typepark",
		"Uncapitalize",
		"unpartial",
		"unshift"
	],
	// "cSpell.ignoreRegExpList": [
	//   "&.*;"
	// ],
	"eslint.enable": true,
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/.hg/store/**": true,
		"**/.progress/**": true,
		"**/node_modules/*/**": true,
		".github": true,
		"cjs": true,
		"coverage": true,
		"esm": true,
		"scripts": true
	},
	"json.schemas": [
		{
			"fileMatch": ["tslint.json"],
			"url": "http://json.schemastore.org/tslint"
		},
		{
			"fileMatch": ["tsconfig.*.json"],
			"url": "http://json.schemastore.org/tsconfig"
		}
	],
	"livePreview.defaultPreviewPath": "/apps/website/dist/index.html",
	"livePreview.serverRoot": "/apps/website/dist",
	"markdown.extension.list.toggle.candidate-markers": ["-", "1."],
	"markdown.extension.toc.levels": "2..6",
	"markdown.extension.toc.omittedFromToc": {
		"type-plus/README.md": ["## Table of Contents", "## Installation"]
	},
	"markdown.extension.toc.orderedList": true,
	"markdown.extension.toc.unorderedList.marker": "-",
	"material-icon-theme.files.associations": {
		"tsconfig.ts54.json": "tsconfig",
		"tsconfig.ts55.json": "tsconfig",
		"tsconfig.ts56.json": "tsconfig"
	},
	"tsimporter.filesToExclude": ["**/cjs", "**/esm", "**/tslib"],
	"typescript.tsdk": "node_modules/typescript/lib"
}
